  <script type="text/javascript">
	var locationBrands = "/catalog/brands?view=brands&limit=50";
	var locationClass = "/catalog?view=class&limit=50";
	var locationAllItems =  "/catalog/allitems?view=allitems&limit=50";
	function changeFunc() {
    var selectview = document.getElementById("selectview");
    var selectedValue = selectview.options[selectview.selectedIndex].value;
	if(selectedValue == "bybrand") {
	window.location = locationBrands }
	if(selectedValue == "byclass") {
	window.location = locationClass }
	if(selectedValue == "allitems") {
	window.location = locationAllItems }
   }
  </script>
