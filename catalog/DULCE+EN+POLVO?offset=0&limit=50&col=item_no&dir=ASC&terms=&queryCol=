function addItem(id, desc, brand, currprice, currpriceunit, qty, size, ums) {
	var frmId
        frmId = document.getElementById('form-id');
	frmId.value = id;

	frmId = document.getElementById('form-desc');
	frmId.value = desc;

        frmId = document.getElementById('form-brand');
	frmId.value = brand;

        frmId = document.getElementById('form-currprice');
	frmId.value = currprice;

        frmId = document.getElementById('form-currpriceunit');
	frmId.value = currpriceunit;

        frmId = document.getElementById('form-qty');
	frmId.value = qty;

        frmId = document.getElementById('form-size');
	frmId.value = size;

        frmId = document.getElementById('form-ums');
	frmId.value = ums;

	var form = document.getElementById('myform');
	form.submit();
}

</script>
